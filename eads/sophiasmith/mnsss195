               <unittitle>Aquiles Sobrero collection of additional items regarding the Margaret Sanger Research Bureau</unittitle>
               <unitid>2017-S-0068</unitid>
               <origination label="source">
                  <persname authfilenumber="people_5283"
                            role="dnr"
                            source="local_inmagic-people">Sobrero, Adela M.</persname>
               </origination>
